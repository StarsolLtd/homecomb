{
    "name": "starsol/homecomb",
    "description": "The HomeComb application",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "doctrine/annotations": "^1.11",
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/doctrine-migrations-bundle": "^2.2",
        "doctrine/orm": "^2.7",
        "easycorp/easyadmin-bundle": "^3.1",
        "google/recaptcha": "^1.2",
        "guzzlehttp/guzzle": "^6.0",
        "knpuniversity/oauth2-client-bundle": "^2.8",
        "league/oauth2-google": "^4.0",
        "phpspec/prophecy": "^1.12",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^9.4",
        "scienta/doctrine-json-functions": "^4.2",
        "sensio/framework-extra-bundle": "^5.6",
        "stof/doctrine-extensions-bundle": "^1.5",
        "symfony/console": "5.3.*",
        "symfony/dotenv": "5.3.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.3.*",
        "symfony/framework-bundle": "5.3.*",
        "symfony/http-client": "5.3.*",
        "symfony/http-foundation": "5.3.*",
        "symfony/mailer": "5.3.*",
        "symfony/monolog-bundle": "^3.6",
        "symfony/phpunit-bridge": "^5.1",
        "symfony/property-access": "5.3.*",
        "symfony/routing": "5.3.*",
        "symfony/security-bundle": "5.3.*",
        "symfony/sendgrid-mailer": "5.3.*",
        "symfony/serializer": "5.3.*",
        "symfony/twig-bundle": "5.3.*",
        "symfony/validator": "5.3.*",
        "symfony/webpack-encore-bundle": "^1.8",
        "symfony/yaml": "5.3.*",
        "symfonycasts/reset-password-bundle": "^1.1",
        "symfonycasts/verify-email-bundle": "^1.1",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0",
        "vich/uploader-bundle": "^1.16"
    },
    "require-dev": {
        "composer/package-versions-deprecated": "1.11.99.1",
        "dama/doctrine-test-bundle": "^6.5",
        "dbrekelmans/bdi": "^0.3.0",
        "dg/bypass-finals": "^1.3",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpmd/phpmd": "^2.10",
        "phpstan/phpstan": "^0.12.57",
        "symfony/browser-kit": "5.3.*",
        "symfony/css-selector": "5.3.*",
        "symfony/debug-bundle": "5.3.*",
        "symfony/maker-bundle": "^1.23",
        "symfony/panther": "^0.9.0",
        "symfony/stopwatch": "^5.0",
        "webmozart/assert": "^1.9"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "stan": "vendor/bin/phpstan analyse -c phpstan.neon src --level max",
        "test-unit": "vendor/bin/phpunit tests/Unit"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.3.*"
        }
    }
}
